@charset "UTF-8";

html, body, div, span, iframe, h1, h2, h3, h4, h5, h6, p, em, img, small, strong, var, i, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, figcaption, figure, footer, header, hgroup, menu, nav, section, summary, time, mark, audio, video {
	margin: 0;
	padding: 0;
	border: 0;
	outline: 0;
	font-size: 100%;
	font-style: normal;
	font-weight: normal;
	vertical-align: baseline;
	background: transparent;
}
article, aside, figcaption, figure, footer, header,  menu, nav, section {
	display: block;
}
ul {
	list-style: none;
}
a {
	margin: 0;
	padding: 0;
	font-size: 100%;
	vertical-align: baseline;
	background: transparent;
	text-decoration: none;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
input, select {
	vertical-align: middle;
}
input:focus,
button:focus,
select:focus {
	outline: none;
}
img {
	max-width: 100%;
	height: auto;
	border: none;
	vertical-align: middle;
}
body {
	width: 100%;
	height: 100vh;
	background: url('../images/base.png') 50% 50% no-repeat fixed;
	background-size: cover;
	font-family: "Hiragino Kaku Gothic ProN","メイリオ", sans-serif;
	line-height: 1.6;
	color: #fff;
}
input, button, select, textarea {
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
}
/************************************ サイト幅*/
.container {
	margin: 20px 15px;
}
.mg-width-00 {
	margin: 20px 0 0;
}
.mg-height-00 {
	margin: 0;
}
/************************************** ヘッダー*/
header {
	position: fixed;
	top: 0;
	width: 100%;
	box-shadow:0px 0px 17px 4px #061131;
	-moz-box-shadow:0px 0px 17px 4px #061131;
	-webkit-box-shadow:0px 0px 17px 4px #061131;
	z-index: 100;
}
header > .head_main {
	min-height: 62px;
	background: #d2d5de;
	display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
 	-webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    -webkit-justify-content: space-between;
    justify-content: space-between;
	padding: 10px;
}
header > .head_main > .mypage,
header > .head_main > .setting,
header > .head_main > .btn_back,
header > .head_main > .btn_head_common {
	width: 15%;
}
header > .head_main > .mypage,
header > .head_main > .setting {
	text-align: right;
}
header > .head_main > .btn_back {
	text-align: left;
	color: #8e96ac;
}
header > .head_main > .btn_head_common {
	border-radius: 3px;
	font-size: 70%;
	color: #fff;
	text-align: center;
	line-height: 1;
	padding: 6px;
}
header > .head_main > .btn_head_common::before {
	display: block;
	content: '';
	font-family: FontAwesome;
	font-size: 130%;
	margin-bottom: 5px;
}
/*指示取消ボタン*/
header > .head_main > .cancel {
	background: #e94050;
}
header > .head_main > .cancel::before {
	content: '\f057';
}
/*合流ボタン*/
header > .head_main > .confluence {
	background: #2ac278;
}
header > .head_main > .confluence::before {
	content: '\f1e0';
}
/*更新ボタン*/
header > .head_main > .reload {
	background: #2ac278;
}
header > .head_main > .reload::before {
	content: '\f021';
}
header > .head_main > h1 {
	width: 70%;
	padding: 0 40px;
}
/************************************** コンテンツ*/
.menu_contents {
	height: 100%;
}
.height-auto {
	height: auto;
}
/*見出しエリア*/
.area_infomation {
	width: 100%;
	font-size: 110%;
	background: #1a2c59;
	padding: 15px;
}
.border_vert {
	border: solid rgba(255, 255, 255, .3);
	border-width: 1px 0 1px 0;
}
.area_infomation > h2 {
	height: 50px;
	line-height: 50px;
	padding: 0 20px;
	text-align: center;
}
.progress {
	display: table;
	width: 100%;
	height: 50px;
	font-size: 90%;
}
.progress div {
	display: table-cell;
	vertical-align: middle;
	text-align: center;
	position: relative;
}
.progress div:first-of-type {
	width: 65%;
}
.progress div:last-of-type {
	background: rgba(255, 255, 255, .2);
	font-size: 80%;
	color: rgba(255, 255, 255, .5);
	padding: 0 10px 0 20px;
}
.progress div:last-of-type::after {
	border-left: solid 10px #1a2c59;
    border-top: solid 25px transparent;
    border-bottom: solid 25px transparent;
    content: "";
    position: absolute;
    left: 0;
    top: 0;
}
.progress_finish {
	display: block;
	height: 50px;
	line-height: 50px;
	text-align: center;
}
/*取得データ表示ボックス*/
.result_contents > section {
	display: table;
	width: 100%;
    background: rgba(255, 255, 255, .2);
	border: solid 3px rgba(255, 255, 255, .1);
	border-radius: 5px;
	padding: 15px 10px;
	margin-bottom: 20px;
}
.result_contents > section:last-of-type {
	margin-bottom: 0;
}
.result_contents > section h2,
.result_contents > section p {
	display: table-cell;
	vertical-align: middle;
}
.result_contents > section h2 {
	position: relative;
	width: 35%;
}
.result_contents > section h2::after {
	position: absolute;
	top: 50%;
	right: 0;
	transform: translateY(-50%);
	-webkit- transform: translateY(-50%);
	content: '\f105';
	font-family: FontAwesome;
	font-size: 130%;
	font-weight: bold;
	color: #15bbde;
	margin-left: 10px;
}
.result_contents > section > p {
	text-align: center;
	overflow: hidden;
}
/*メインリスト*/
.step_box {
	display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    flex-wrap: wrap;
	height: 100%;
	background: rgba(255, 255, 255, .1);
}
.step_box div {
	width: 50%;
	height: 20%;
	border-bottom: solid 1px rgba(255, 255, 255, .2);
	color: #fff;
	padding: 15px;
}
.step_box div:nth-of-type(1),
.step_box div:nth-of-type(2) {
	padding-top: 20px;
}
.step_box div:last-of-type,
.step_box div:nth-last-of-type(2) {
	border-bottom: none;
}
.step_box div:nth-child(odd) {
	border-right: solid 1px rgba(255, 255, 255, .2)
}
/*メインリスト　アクティブ*/
.step_box div.active {
	z-index: 2;
	overflow: hidden;
	background-color: #2ac278;
	transition: .3s linear;
	-webkit-transition: .3s linear;
	-moz-transition: .3s linear;
}
.step_box dl dt,
.step_box dl dd {
	text-align: center;
	vertical-align: middle;
}
.step_box dl dt {
	margin-bottom: 8px;
}
.step_box dl dt img {
	width: 30px;
	height: 30px;
}
/*ステップ*/
.step_info {
	background: #556183;
	color: #fff;
}
.step_info h2 {
	background: #8e96ac;
	border-bottom: solid 3px rgba(0, 0, 0, .1);
	font-size: 120%;
	padding: 10px 15px;
}
.step_info img {
	width: 50px;
	height: 50px;
}
.step_info figcaption {
	font-weight: bold;
	margin: 5px 0 20px;
}
.step_info p {
	border-top: solid 1px rgba(255, 255, 255, .2);
	padding: 15px;
}
.border-top {
	border-top: solid 1px rgba(255, 255, 255, .2);
}

/*リストボックス*/
.list_box_type01 section,
.list_box_type03 dl {
	position: relative;
	display: table;
	width: 100%;
	border-bottom: solid 1px #9198ac;
	padding: 10px 40px 10px 10px;
	overflow: hidden;
}

.list_box_type01 section:first-of-type,
.list_box_type03 dl:first-of-type {
	border-top: solid 1px #9198ac;
}
.list_box_type01 section::after,
.list_box_type03 dl::after,
.list_box_type03 div::after {
	position: absolute;
	top: 50%;
	right: 10px;
	margin-top: -20px;
	content: '\f105';
	font-family: FontAwesome;
	font-size: 150%;
	color: #15bbde;
}
.list_box_type01 section div {
	display: table-cell;
	vertical-align: middle;
}
.list_box_type01 section div.status {
	width: 40px;
	text-align: center;
	padding-right: 10px;
}
.list_box_type01 section div.status img {
	width: 30px;
	height: 30px;
}
.list_box_type01 section div.inner_contents {
	position: relative;
}
.list_box_type01 section div.inner_contents h2 {
	font-weight: bold;
}
.list_box_type01 section div.inner_contents p {
	font-size: 90%;
}
/*完了時　共通*/
.list_box_type01 section.complete {
	background: rgba(21, 187, 222, .5);
	color: #15bbde;
}
/*返品時*/
.list_box_type01 section.cancel {
	background: rgba(216, 180, 34, .5);
	color: #d8b422;
}
.list_box_type01 section.cancel::after {
	color: #d8b422;
}
/*除外時*/
.list_box_type01 section.exception {
	background: rgba(26, 44, 89, .5);
	color: #606b8a;
}
.list_box_type01 section.exception::after {
	color: #606b8a;
}
/*不在時*/
.list_box_type01 section.absence {
	background: rgba(233, 64, 80, .5);
	color: #e94050;
}
.list_box_type01 section.absence::after {
	color: #e94050;
}
/*荷物特記*/
.cargo_info {
	position: absolute;
	top: 0;
	right: 0;
}
.cargo_info li {
	display: inline-block;
}
.cargo_info li img {
	width: 15px;
	height: 15px;
}
.list_box_type02 dl {
	display: table;
	width: 100%;
	border-bottom: solid 1px rgba(255, 255, 255, .5);
	padding: 10px 0;
}
.list_box_type02 dl:last-of-type {
	border-bottom: none;
}
.list_box_type02 dt,
.list_box_type02 dd {
	display: table-cell;
	vertical-align: middle;
	padding: 0 10px;
}
.list_box_type02 dt {
	width: 35%;
}
.list_box_type02 .checked {
	font-size: 120%;
	color: #2ac278;
}
.list_box_type02 .unfinished {
	font-size: 120%;
	color: #ccc;
}
.list_box_type02 .ico_result li {
	display: inline-block;
	margin-right: 30px;
}
.list_box_type02 .ico_result li:first-of-type::before {
	content: '\f041';
	font-family: FontAwesome;
	color: #2ac278;
	margin-right: 5px;
}
.list_box_type02 .ico_result li:last-of-type::before {
	content: '\f1b2';
	font-family: FontAwesome;
	color: #d8b422;
	margin-right: 5px;
}
.list_box_type03 dl {
	display: table;
	width: 100%;
	background: #525e80;
}
.list_box_type03 dl dt,
.list_box_type03 dl dd {
	display: table-cell;
	vertical-align: middle;
}
.list_box_type03 dl dt {
	width: 35%;
}
.list_box_type03 dl dd {
	text-align: right;
}
.list_box_type03 p {
	font-size: 80%;
	padding: 5px 10px;
}
.no-arrow section::after,
.no-arrow dl::after {
	position: absolute;
	display: none;
}
.no-arrow dl {
	padding: 10px !important;
}
.list_box_type03 div {
	position: relative;
	display: block;
	width: 100%;
	background: #525e80;
	border: solid #9198ac;
	border-width: 1px 0;
	padding: 10px 40px 10px 10px;
	overflow: hidden;
}
/*カメラ*/
.area_photo {
	display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
 	-webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    -webkit-justify-content: space-between;
    justify-content: space-between;
}
.area_photo > div {
	position: relative;
	width: 30%;
}
.area_photo button {
	position: absolute;
	top: 5px;
	right: 5px;
	width: 20px;
	height: 20px;
	background: url('../images/btn_close.png');
	background-size: contain;
	z-index: 50;
}
.point_photo h2 {
	width: 120px;
	line-height: 1;
	background: #d2d5de;
	color: #1a2c59;
	font-weight: bold;
	padding: 7px 0;
	border-radius: 50px;
	text-align: center;
	margin: 0 auto;
}
.point_photo h2::before {
	content: '\f0a4';
	font-family: FontAwesome;
	font-size: 130%;
	font-weight: normal;
	margin-right: 6px;
}
.point_photo p {
	margin: 0 15px;
}
.sign_view {
	background: #fff;
	border: solid rgba(0, 0, 0, .1);
	border-width: 8px 0 8px 0;
	padding: 10px;
}
.sign_view h2 {
	position: relative;
	color: rgba(0, 0, 0, .7);
	border-bottom: solid 1px rgba(0, 0, 0, .1);
	padding-bottom: 5px;
}
.sign_view h2::after {
	position: absolute;
	top: 0;
	bottom: 0;
	right: 0;
	margin: auto;
	content: 'Signature';
	font-size: 80%;
	color: rgba(0, 0, 0, .2);
}
.sign_view figure {
	padding: 20px;
}
.sign_view img {
	width: 60%;
	margin: auto;
}
/*モーダル*/
.modal-open {
	cursor: pointer;
}
#modal-content {
	position: relative;
	width: 100%;
	max-width: 300px;
	background: #d2d5de;
	box-shadow:0px 0px 15px 0px #000000;
	-moz-box-shadow:0px 0px 15px 0px #000000;
	-webkit-box-shadow:0px 0px 15px 0px #000000;
	color: #333;
	margin: 10px;
	z-index: 9999;
}
#modal-overlay {
	top: 0;
	left: 0;
	width: 100%;
	height: 120%;
	background:	rgba(0, 0, 0, 0.7);
	z-index: 9998;
}
#modal-content,
#modal-overlay {
	display: none;
	position: fixed;
}
#modal-content section h2 {
	font-size: 130%;
	color: rgba(0, 0, 0, .9);
	padding: 10px 20px;
	border-bottom: solid 1px rgba(0, 0, 0, .05);
}
#modal-content section p {
	font-size: 110%;
	padding: 20px;
	color: rgba(0, 0, 0, .6);
}
/*お知らせエリア*/
#modal-content section div {
	height: 60vh;
	padding: 10px 20px 20px;
	overflow: scroll;
}
#modal-content section div dl {
	padding: 10px 0;
	border-bottom: solid 1px rgba(0, 0, 0, .2);
}
#modal-content section div dl:last-of-type {
	border-bottom: none;
}
#modal-content section div dl dt {
	font-weight: bold;
	color: rgba(0, 0, 0, .4);
	margin-bottom: 5px;
}
#modal-content section div dl dd {
	line-height: 1.4;
}
/*モーダルボタン*/
.modal-ok {
	background: #2ac278;
	color: #fff;
	padding: 15px;
	cursor: pointer;
}
#modal-close {
	display: block;
	position: absolute;
	top: 12px;
	right: 10px;
	width: 30px;
	height: 30px;
	cursor: pointer;
}
.btn_col-2 {
	display: table;
	width: 100%;
}
.btn_col-2 div {
	display: table-cell;
	width: 50%;
	color: #fff;
	padding: 15px;
	cursor: pointer;
}
.btn_col-2 div:first-of-type {
	background: #e94050;
}
.btn_col-2 div:last-of-type {
	background: #2ac278;
}
/*合流画面*/
.confluence_select {
	display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
 	-webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    flex-wrap: wrap;
}
.confluence_select div {
	width: 48%;
	text-align: center;
	border-radius: 5px;
	padding: 20px;
	font-size: 130%;
}
.confluence_select div::before {
	display: block;
	content: '';
	font-family: FontAwesome;
	margin-bottom: 10px;
}
.confluence_select div:first-of-type {
	background: #ff0000;
	border: solid 4px #ffffff;
}
.confluence_select div:first-of-type::before {
	content: '\f08b';
}
.confluence_select div:last-of-type {
	background: rgba(42, 194, 120, .8);
	border: solid 4px #2ac278;
}
.confluence_select div:last-of-type::before {
	content: '\f090';
}
/************************************** フッター*/
footer {
	width: 100%;
	position: fixed;
	right: 0;
	left: 0;
	bottom: 0;
	z-index: 100;
	box-shadow: 0px -5px 21px -7px #061131;
	-moz-box-shadow: 0px -5px 21px -7px #061131;
	-webkit-box-shadow: 0px -5px 21px -7px #061131;
	text-align: center;
}
/*ページ下部ボタン*/
.common_btn {
	width: 100%;
	display: table;
	background: #2ac278;
}
.common_btn > div {
	display: table-cell;
	width: 100%;
	vertical-align: middle;
	font-size: 100%;
	text-align: center;
	padding: 15px;
}
.common_btn > div::before {
	display: block;
    font-size: 100%;
    font-family: FontAwesome;
}

/*配送作業開始時*/
.bottom_btn > div {
	display: table-cell;
	width: 25%;
	background: #e4e6ea;
	border-right: solid 1px #d2d5de;
	vertical-align: middle;
	font-size: 100%;
	color: #8e96ac;
	text-align: center;
	padding: 15px;
	transition: .3s;
}
.bottom_btn > div:last-of-type {
	border-right: none;
}
.bottom_btn > div:nth-of-type(1) {
	color: #e94050;
}
.bottom_btn > div:nth-of-type(2) {
	color: #2ac278;
}
.bottom_btn > div:nth-of-type(3) {
	color: #d8b422;
}
.bottom_btn > div:nth-of-type(4) {
	color: #15bbde;
}
.bottom_btn > div:nth-of-type(1)::before {
    content: '\f00d';
}
.bottom_btn > div:nth-of-type(2)::before {
    content: '\f030';
}
.bottom_btn > div:nth-of-type(3)::before {
    content: '\f040';
}
.bottom_btn > div:nth-of-type(4)::before {
    content: '\f1d8';
}

/*設定画面用*/
.bottom_setting_btn > div {
	display: table-cell;
	width: 50%;
	background: #e4e6ea;
	vertical-align: middle;
	font-size: 100%;
	color: #8e96ac;
	text-align: center;
	padding: 15px;
}
.bottom_setting_btn > div:first-of-type {
	border-right: solid 1px #d2d5de;
}
.bottom_setting_btn > div:last-of-type {
	position: relative;
}
.bottom_setting_btn > div:last-of-type > div,
.bottom_news_btn > div:last-of-type > div {
	position: absolute;
	top: 5px;
	right: 10px;
	display: inline-block;
	height: 26px;
	line-height: 26px;
	min-width: 26px;
	border-radius: 26px;
	padding: 0 5px;
	background: #e94050;
	font-size: 12px;
	color: #fff;
	text-align: center;
}
.bottom_setting_btn > div::before {
	display: block;
    font-size: 100%;
    content: '';
    font-family: FontAwesome;
}
.bottom_setting_btn > div:nth-of-type(1)::before {
    content: '\f05a';
}
.bottom_setting_btn > div:nth-of-type(2) {
    color: #e94050;
}
.bottom_setting_btn > div:nth-of-type(2)::before {
    content: '\f071';
}
/*未送信ボタンのみ*/
.btn_unsent > div {
	position: relative;
	background: #e4e6ea;
	color: #e94050;
}
.btn_unsent > div::before {
	content: '\f071';
}
/*件数*/
.btn_unsent > div > div {
	position: absolute;
	top: 5px;
	right: 10px;
	display: inline-block;
	height: 26px;
	line-height: 26px;
	min-width: 26px;
	border-radius: 26px;
	padding: 0 5px;
	background: #e94050;
	font-size: 12px;
	color: #fff;
	text-align: center;
}

/*合流時*/
.bottom_confluence_btn > div {
	display: table-cell;
	width: 25%;
	vertical-align: middle;
	font-size: 100%;
	text-align: center;
	padding: 15px;
	transition: .3s;
}
.bottom_confluence_btn > div:first-of-type {
	background: #d8b422;
}
.bottom_confluence_btn > div:last-of-type {
	background: #2ac278;
}
.bottom_confluence_btn > div::before {
	display: block;
    font-size: 100%;
    content: '';
    font-family: FontAwesome;
}
.bottom_confluence_btn > div:first-of-type::before {
	content: '\f056';
}
.bottom_confluence_btn > div:last-of-type::before {
	content: '\f055';
}

/*報告*/
.btn_report > div::before,
.bottom_news_btn > div:nth-of-type(1)::before {
	content: '\f1d8';
}
/*受信*/
.btn_dl > div::before {
