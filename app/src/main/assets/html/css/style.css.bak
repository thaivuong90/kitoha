/*キャンセル理由、返品理由選択*/
.select_cancel {
	width: 100%;
	display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
}
.select_cancel label {
	position: relative;
	width: 100%;
}
.select_cancel label select {
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	width: 100%;
	border: none;
	background: #fff;
	color: #757575;
	padding: 10px;
	line-height: 1.2;
}
.select_cancel label::after {
	position: absolute;
	top: 50%;
	right: 15px;
	margin-top: -16px;
	content: '\f107';
	font-size: 120%;
	font-family: FontAwesome;
	color: #ccc;
	pointer-events: none;
}
.select_cancel button {
	background: #2ac278;
	color: #fff;
	font-weight: bold;
    text-align: center;
    padding: 10px 15px;
    line-height: 1.2;
}
